Dashboards

<%= button_for 'Follow', follow_user_path %>

<%= form_for @text_shout do |form| %>
	<%= form.text_field :body, placeholder: 'Shout content here' %>
	<%= form.submit 'Shout' %>
<% end %>

<%= form_for @photo_shout do |form| %>
	<%= form.file_field :image %>
	<%= form.submit 'Shout' %>
<% end %>

<%= render @shouts %>